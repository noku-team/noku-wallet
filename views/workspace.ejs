<div class="hidden js-origin-wallet-card">
  <div class="wallet-card" data-address="{{address}}" data-name="{{wallet_name}}" data-order="{{order}}">
    <div class="buttons">
      <div class="move"></div>
      <div class="close"></div>
    </div>
    <div class="overflow">
      <div class="background-qr">
        <div class="title-wallet js-wallet-name-ws">{{wallet_name}}</div>
        <div class="close-bg"></div>
        <div class="js-qr-code">
          <!-- <img src="" alt=""> -->
        </div>
      </div>
      <div class="card-header flex-container justify-start">
        <div class="card-background" {{background_blockie}}></div>
        <div class="card-avatar">
          {{blockie}}
          <i class="fal fa-qrcode"></i>
        </div>
        <div class="card-dates">
          <div class="card-name js-wallet-name-ws">
            <span class="workspace-wallet-name-plain">{{wallet_name}}</span>
            <input type="text" class="form-control input-wallet-workspace js-wallet-name-input" placeholder="insert wallet name here" value="{{wallet_name}}">
          </div>
          <div class="address">
            <span class="js-address-to-cut">{{address}}</span>
            <div class="copy"></div>
          </div>
        </div>
      </div>
      <div class="card-body">
        <div class="flex-container space-between total-balance">
          <div class="card-label">Total Balance</div>
          <div class="js-total-balance">
          </div>
        </div>
        <div class="token-balance">
          <div class="list-token js-list-token hidden">
            <a class="other-tokens hidden" href="/address/{{address}}">+ <span class="js-total-tokens">{{n_tokens}}</span> other tokens</a>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="hidden js-token-card-original">
  <div>
    {{img_token}}
    <span>{{balance_token}}</span>
  </div>
</div>

