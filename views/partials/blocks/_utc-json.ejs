<div class="modal fade modal-noku not-progress" id="utc-json">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title">Import your encrypted Private Key</h5>
        <i class="far fa-times" data-dismiss="modal"></i>
      </div>
      <div class="modal-body">
        <div class="form-noku">
          <div class="simple-modal">
            <div class="row">
              <div class="col-md-12">
                <div class="title-step">Select Your Wallet File </div>
                <div class="desc-modal">Select the UTC/JSON file of the wallet you want to import</div>
                <form action="" class="">
                  <div class="dropzone-container">
                    <div class="dropzone-element">
                      <div class="icon icon-upload"></div>
                      <div class="span">Drag here or <span>browse</span> to upload</div>
                    </div>
                    <div class="images-preview">
                      <div class="hidden-flex"></div>
                      <div class="hidden-flex"></div>
                      <div class="hidden-flex"></div>
                      <div class="hidden-flex"></div>
                      <div class="hidden-flex"></div>
                      <div class="hidden-flex"></div>
                      <div class="hidden-flex"></div>
                      <div class="hidden-flex"></div>
                      <div class="hidden-flex"></div>
                      <div class="hidden-flex"></div>
                    </div>
                  </div>
                  <div class="label-pk">Password to unlock</div>
                  <input class="password-noku" type="password" placeholder="Enter your password" />
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="modal-footer">
        <div class="pointer mr-auto" data-dismiss="modal">Cancel</div>
        <div class="btn btn-noku dark-blue enc-unlock" data-id="encprivatekey">Unlock</div>
      </div>
    </div>
  </div>
</div>
