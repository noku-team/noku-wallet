<div class="modal fade modal-noku not-progress" id="mnemonicphrase">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title">Import your Mnemonic Phrase</h5>
        <i class="far fa-times" data-dismiss="modal"></i>
      </div>
      <div class="modal-body">
        <div class="form-noku">
          <div class="simple-modal">
            <div class="row">
              <div class="col-md-12">
                <form action="javascript:;" class="validateSubmit js-mnemonicphrase">
                  <%- partial('../content/_mnemonicphrase-content') %>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="modal-footer">
        <div class="pointer mr-auto" data-dismiss="modal">Cancel</div>
        <div class="btn btn-noku dark-blue enc-unlock" data-id="encprivatekey">Unlock</div>
      </div>
    </div>
  </div>
</div>
